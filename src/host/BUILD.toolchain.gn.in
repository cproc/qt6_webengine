import("//build/config/sysroot.gni")
import("//build/toolchain/gcc_toolchain.gni")
gcc_toolchain("@GN_TOOLCHAIN@") {
  cc = "@CMAKE_C_COMPILER@"
  cxx = "@CMAKE_CXX_COMPILER@"
  ld = "@CMAKE_CXX_COMPILER@"
  ar = "@CMAKE_AR@"
  nm = "@CMAKE_NM@"
  extra_cppflags = "-isystem /usr/local/include"
  extra_cxxflags = "-O2 -pipe -fstack-protector-strong -isystem /usr/local/include -fno-strict-aliasing  -isystem /usr/local/include "
  extra_ldflags = " -Wl,--undefined-version -fstack-protector-strong -L/usr/local/lib "
  toolchain_args = {
    current_os = "@GN_OS@"
    current_cpu = "@GN_CPU@"
    v8_current_cpu = "@GN_V8_CPU@"
    is_clang = @GN_IS_CLANG@
    use_gold = false
  }
}
